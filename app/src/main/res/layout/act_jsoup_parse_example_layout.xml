<?xml version="1.0" encoding="utf-8"?>
<layout>

    <data>

    </data>


    <android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <TextView
            android:id="@+id/tv_url_content"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@color/blue_6ac2fb"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_url" />

        <Button
            android:id="@+id/bt_parse"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:text="解析"
            app:layout_constraintLeft_toRightOf="@+id/et_url"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/et_url"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_marginRight="@dimen/margin_15dp"
            app:layout_constraintBottom_toTopOf="@id/tv_url_content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@id/bt_parse"
            app:layout_constraintTop_toTopOf="parent" />

    </android.support.constraint.ConstraintLayout>
</layout>
